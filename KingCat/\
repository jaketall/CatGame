using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public class PowerUpSpawner : MonoBehaviour
{
    public GameObject powerUpPrefab;
    public Color speedSparkleColor;
    public Color speedLightColor;

    GameObject spawnSpeedPowerUp(Vector3 position)
    {
        powerUpPrefab.GetComponentInChildren<Renderer>().material.color = speedColorOverride;
        return Instantiate(powerUpPrefab, position, new Quaternion(0,0,0,0));
    }

    // Start is called before the first frame update
    void Start()
    {
    }

    // Update is called once per frame
    void Update()
    {
        
    }
}
